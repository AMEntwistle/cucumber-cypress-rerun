{
  "name": "cucumber-cypress-rerun",
  "version": "0.0.0-development",
  "description": "A plugin to run cypress cucumber scenarios multiple times",
  "bin": {
    "cucumber-cypress-repeat": "index.js"
  },
  "files": [
    "index.js"
